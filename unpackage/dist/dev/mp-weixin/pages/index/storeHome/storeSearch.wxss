.content {
  padding: 0 32rpx;
  background: #FFFFFF;
  height: 100%;
}
.content .search_box {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding-top: 20rpx;
}
.content .search_box .search-box {
  width: 598rpx;
  height: 78rpx;
  position: relative;
  border-radius: 36rpx;
  background: #F4F4F4;
}
.content .search_box .search-box .search-icon {
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 20rpx;
  left: 20rpx;
}
.content .search_box .search-box .search-icon image {
  width: 100%;
  height: 100%;
}
.content .search_box .search-box .search-input {
  width: 446rpx;
  height: 100%;
  line-height: 78rpx;
  border: none;
  outline: none;
  margin-left: 62rpx;
  font-size: 28rpx;
}
.content .search_box .search-box .cancel-icon {
  position: absolute;
  width: 78rpx;
  height: 100%;
  top: 0;
  right: 0;
}
.content .search_box .search-box .cancel-icon image {
  float: left;
  width: 24rpx;
  height: 24rpx;
  margin: 24rpx 34rpx 0 30rpx;
}
.content .search_box .search-title {
  line-height: 78rpx;
  font-size: 28rpx;
  color: #222222;
}
.content .history-box {
  margin-top: 48rpx;
}
.content .history-box .history-header {
  height: 48rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.content .history-box .history-header .history-title {
  font-size: 28rpx;
  font-weight: 500;
  color: #222222;
  line-height: 48rpx;
}
.content .history-box .history-header .delete-icon {
  width: 48rpx;
  height: 48rpx;
}
.content .history-box .history-header .delete-icon image {
  width: 100%;
  height: 100%;
}
.content .history-box .history-list .list-item {
  float: left;
  padding: 12rpx 36rpx;
  background: #F4F4F4;
  border-radius: 30rpx;
  margin: 32rpx 48rpx 0 0;
  font-size: 28rpx;
  color: #222222;
  max-width: 240rpx;
  overflow: hidden;
  /*溢出隐藏*/
  white-space: nowrap;
  /*文字不换行*/
  text-overflow: ellipsis;
  /*溢出用省略号代替*/
}
.content .search-list .noMore {
  padding: 200rpx 0;
  text-align: center;
  color: #666666;
}
.content .search-list .item-list {
  margin-top: 48rpx;
  font-size: 28rpx;
  color: #222222;
}
.content .search-list .item-list .item-key {
  color: #999999;
}

