<view><view class="s-head"><view class="s-head-cover"></view><view class="s-info"><image src="{{factoryInfo.factory_cover_img}}"></image><view class="s-text"><view class="s-text1">{{factoryInfo.factory_nickname}}</view><view class="s-text2">{{"总销量："+factoryInfo.factory_sales_volume+"万"}}</view></view><view class="s-select"><image src="/static/img/strore/right.png"></image></view></view><block wx:if="{{userInfo.token}}"><button class="p-share" open-type="share"><image class="p-s-img" src="/static/img/strore/share.png"></image><view class="p-s-text">分享</view></button></block><block wx:else><button data-event-opts="{{[['tap',[['shareFn']]]]}}" class="p-share" bindtap="__e"><image class="p-s-img" src="/static/img/strore/share.png"></image><view class="p-s-text">分享</view></button></block><image src="{{factoryInfo.factory_back_img}}"></image><view data-event-opts="{{[['tap',[['toStoreSearch',['$event']]]]]}}" class="s-search" bindtap="__e"><view class="search-box"><view class="search-icon"><image src="/static/img/index/search-icon.png"></image></view><view class="search-input">搜索商品</view></view></view></view><view><scroll-view class="bg-white nav" style="{{'position:'+(isFixd?'fixed':'')+';'+('top:'+(0)+';')+('z-index:'+(isFixd?9999:'')+';')}}" id="tab" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{factoryInfo.type_data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',TabCur==index?'current':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event','$0'],[[['factoryInfo.type_data','',index,'type_id']]]]]]]}}" bindtap="__e"><view class="tab-text" style="{{(TabCur==index?'color: #ED494CFF;':'')}}">{{item.name}}</view><view class="tab-bottom-line-box"><view class="tab-bottom-line" style="{{('background-color: '+themeColor+';')}}"></view></view></view></block></scroll-view></view><view style="margin-top:20rpx;"><scroll-view class="bg-white nav" id="tab" scroll-x scroll-with-animation scroll-