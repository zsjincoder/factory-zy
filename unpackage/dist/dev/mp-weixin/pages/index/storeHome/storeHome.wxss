.nav .cu-item {
  font-size: 32rpx;
}
.nav .cu-item .tab-text {
  height: 86rpx;
}
.nav .cu-item .tab-text1 {
  height: 86rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.nav .cu-item .tab-text1 .t-short {
  width: 30rpx;
  height: 100%;
  position: relative;
}
.nav .cu-item .tab-text1 .t-short.t-short:before {
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  top: 42rpx;
  left: 8rpx;
  border-right: solid 8rpx transparent;
  border-left: solid 8rpx transparent;
  border-bottom: solid 8rpx #999999;
}
.nav .cu-item .tab-text1 .t-short.top::before {
  border-bottom: solid 8rpx #ED494C;
}
.nav .cu-item .tab-text1 .t-short.t-short:after {
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  bottom: 26rpx;
  left: 8rpx;
  border-top: solid 8rpx #999999;
  border-right: solid 8rpx transparent;
  border-left: solid 8rpx transparent;
}
.nav .cu-item .tab-text1 .t-short.bottom::after {
  border-top: solid 8rpx #ED494C;
}
.nav .cu-item .tab-bottom-line {
  width: 0%;
  height: 4rpx;
  background-color: #0081FF;
  -webkit-transition-duration: 0.3s;
          transition-duration: 0.3s;
}
.nav .cu-item .tab-bottom-line-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.nav .cu-item.current {
  margin-bottom: 20rpx;
}
.nav .cu-item.current .tab-bottom-line {
  width: 100%;
}
