.content {
  background: #FFFFFF;
  height: 100%;
}
.content .header-box {
  padding: 48rpx 34rpx;
}
.content .header-box .header-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.content .header-box .header-title .title {
  font-size: 28rpx;
  color: #222222;
}
.content .header-box .header-title .title-img {
  width: 32rpx;
  height: 32rpx;
  margin-top: 2rpx;
}
.content .header-box .header-title .title-img image {
  width: 100%;
  height: 100%;
}
.content .header-box .income-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-top: 32rpx;
}
.content .header-box .income-box .income {
  line-height: 66rpx;
  font-size: 48rpx;
  font-weight: 500;
  color: #222222;
}
.content .header-box .income-box .withdraw {
  width: 140rpx;
  height: 48rpx;
  border: 2rpx solid #ED494C;
  border-radius: 28rpx;
  font-size: 22rpx;
  color: #ED494C;
  line-height: 48rpx;
  text-align: center;
  margin-top: 8rpx;
}
.content .divider {
  width: 100%;
  height: 16rpx;
  background: #F8F8F8;
}
.content .content-box {
  margin: 0 32rpx;
}
.content .content-box .item {
  padding: 34rpx 0;
  border-bottom: 2rpx solid #EEEEEE;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.content .content-box .item .item-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.content .content-box .item .item-info .item-title,
.content .content-box .item .item-info .info {
  font-size: 28rpx;
}
.content .content-box .item .item-info .green {
  color: #4CBF00;
}
.content .content-box .item .item-info .item-scripts {
  font-size: 24rpx;
  color: #9E9E9E;
  max-width: 50%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.content .content-box .item .item-info .item-time {
  font-size: 24rpx;
  color: #9E9E9E;
}
.content .content-box .item .info-sec {
  margin-top: 10rpx;
}
.content .rules-box {
  width: 568rpx;
  height: 756rpx;
}
.content .rules-box .rules {
  background: #FFFFFF;
  width: 100%;
  height: 660rpx;
  padding: 46rpx;
}
.content .rules-box .rules .scroll-Y {
  width: 100%;
  height: 100%;
}
.content .rules-box .close-box {
  margin: 48rpx auto 0;
  width: 48rpx;
  height: 48rpx;
}
.content .rules-box .close-box image {
  width: 100%;
  height: 100%;
}

