<view class="content"><view class="header-box"><view class="header-title"><view class="title">可提现金额（元）</view><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="title-img" bindtap="__e"><image src="../../../static/img/mine/info.png"></image></view></view><view class="income-box"><view class="income">{{moneys}}</view><view data-event-opts="{{[['tap',[['toWithdrawal',['$event']]]]]}}" class="withdraw" bindtap="__e">提现</view></view></view><view class="divider"></view><view class="content-box"><block wx:for="{{incomeList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="item"><view class="item-info"><view class="item-title">{{item.title}}</view><view class="{{['info',item.type===1?'':'green']}}"><block wx:if="{{item.type===1}}"><text>+</text></block><block wx:else><text>-</text></block>{{''+item.moeny+''}}</view></view><view class="item-info info-sec"><view class="item-scripts">{{item.desc}}</view><view class="item-time">{{item.create_at}}</view></view></view></block><uni-load-more vue-id="9989d08c-1" status="{{loadingType}}" bind:__l="__l"></uni-load-more></view><uni-popup class="vue-ref" vue-id="9989d08c-2" maskClick="{{maskFlag}}" type="center" data-ref="rules" bind:__l="__l" vue-slots="{{['default']}}"><view class="rules-box"><view class="rules"><scroll-view class="scroll-Y" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="rules-content">{{''+rules+''}}</view></scroll-view></view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="close-box" bindtap="__e"><image src="../../../static/img/mine/close.png" mode></image></view></view></uni-popup></view>