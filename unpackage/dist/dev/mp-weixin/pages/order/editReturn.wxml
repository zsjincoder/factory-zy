<view class="content"><view class="divider"></view><block wx:for="{{getReturnOrder.goods}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="product-box"><view class="img-box"><image src="{{item.cover_img}}"></image></view><view class="info-box"><view class="product-title" style="display:flex;justify-content:space-between;"><view>{{item.goods_name}}</view><view>{{"￥"+item.unit_price}}</view></view><view class="product-Specifications" style="display:flex;justify-content:space-between;"><view>{{item.standard_desc}}</view><view>{{"x"+item.num}}</view></view></view></view></block><view class="divider"></view><view class="order-info"><view class="title">订单编号</view><view class="info-box"><view class="order-id">{{getReturnOrder.order_number}}</view><view data-event-opts="{{[['tap',[['copye',['$event']]]]]}}" class="oper" bindtap="__e">复制</view></view></view><view class="divider"></view><view class="my-logistics"><view class="title">快递单号</view><view class="logistics-box"><input class="logistics-input" type="text" placeholder="请输入退货商品的物流单号" placeholder-style="font-size: 28rpx; color: #E5E5E5" data-event-opts="{{[['input',[['getLogistics',['$event']]]]]}}" value="{{logistics}}" bindinput="__e"/></view></view><view class="divider"></view><view class="button-box"><button class="{{['button',disabled?'':'active']}}" disabled="{{disabled}}" data-event-opts="{{[['tap',[['Return']]]]}}" bindtap="__e">提交</button></view></view>